<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="550.0" prefWidth="756.0" stylesheets="@viewerhome.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewer.ViewerHomeController">
   <children>
      <TableView fx:id="tableView" layoutX="4.0" layoutY="117.0" prefHeight="300.0" prefWidth="756.0">
        <columns>
          <TableColumn fx:id="regNumColumn" prefWidth="84.0" text="Reg Num" />
          <TableColumn fx:id="carMakeColumn" prefWidth="84.0" text="Car Make" />
            <TableColumn fx:id="carModelColumn" prefWidth="84.0" text="Car Model" />
            <TableColumn fx:id="color1Column" prefWidth="84.0" text="Color1" />
            <TableColumn fx:id="color2Column" prefWidth="84.0" text="Color2" />
            <TableColumn fx:id="color3Column" prefWidth="84.0" text="Color3" />
            <TableColumn fx:id="yearMadeColumn" prefWidth="84.0" text="Year Made" />
            <TableColumn fx:id="priceColumn" prefWidth="84.0" text="Price" />
            <TableColumn fx:id="quantityColumn" prefWidth="84.0" text="Quantity" />
        </columns>
      </TableView>
      <TextField fx:id="regNumText" layoutX="14.0" layoutY="74.0" onKeyReleased="#regNumTextChanged" promptText="Reg Num">
         <font>
            <Font name="Zilla Slab" size="12.0" />
         </font></TextField>
      <TextField fx:id="carMakeText" layoutX="190.0" layoutY="74.0" onKeyReleased="#carMakeTextChanged" promptText="Car Make">
         <font>
            <Font name="Zilla Slab" size="12.0" />
         </font></TextField>
      <TextField fx:id="carModelText" disable="true" layoutX="363.0" layoutY="74.0" promptText="Car Model">
         <font>
            <Font name="Zilla Slab" size="12.0" />
         </font></TextField>
      <Text layoutX="88.0" layoutY="42.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Viewer">
         <font>
            <Font name="Zilla Slab" size="18.0" />
         </font>
      </Text>
      <Button fx:id="refresh" layoutX="588.0" layoutY="23.0" mnemonicParsing="false" onAction="#refresh" prefHeight="25.0" text="Refresh">
         <font>
            <Font name="Zilla Slab" size="12.0" />
         </font>
         <graphic>
            <FontAwesomeIcon glyphName="REFRESH" />
         </graphic></Button>
      <Button fx:id="back" layoutX="670.0" layoutY="23.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="70.0" text="Back">
         <font>
            <Font name="Zilla Slab Light" size="12.0" />
         </font>
         <graphic>
            <FontAwesomeIcon glyphName="ARROW_LEFT" />
         </graphic></Button>
      <Button fx:id="buyCar" layoutX="345.0" layoutY="466.0" mnemonicParsing="false" onAction="#buyCar" prefHeight="40.0" text="Buy Car">
         <font>
            <Font name="Zilla Slab" size="12.0" />
         </font>
         <graphic>
            <FontAwesomeIcon glyphName="SHOPPING_CART" />
         </graphic></Button>
      <Button fx:id="viewImage" layoutX="644.0" layoutY="430.0" mnemonicParsing="false" onAction="#viewImage" prefHeight="25.0" text="View Image">
         <font>
            <Font name="Zilla Slab Light" size="12.0" />
         </font>
         <graphic>
            <FontAwesomeIcon glyphName="PHOTO" />
         </graphic></Button>
      <Button fx:id="search" layoutX="537.0" layoutY="74.0" mnemonicParsing="false" onAction="#search" prefHeight="25.0" text="Search">
         <font>
            <Font name="Zilla Slab" size="12.0" />
         </font>
         <graphic>
            <FontAwesomeIcon glyphName="SEARCH" />
         </graphic></Button>
      <Button fx:id="reset" layoutX="623.0" layoutY="74.0" mnemonicParsing="false" onAction="#reset" prefHeight="25.0" text="Reset">
         <font>
            <Font name="Zilla Slab" size="12.0" />
         </font>
         <graphic>
            <FontAwesomeIcon glyphName="UNDO" />
         </graphic></Button>
   </children>
</AnchorPane>
